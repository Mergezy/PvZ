<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/DarckDrown"
    tools:context=".achivePage">

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="18dp"
        android:onClick="goToMainPage"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="542dp"
        android:layout_height="111dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="20dp"
        android:background="@color/DarckDrown"
        android:onClick="goToPlantPage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        app:srcCompat="@drawable/achivcharry" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="410dp"
        android:layout_height="111dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="20dp"
        android:background="@color/DarckDrown"
        android:onClick="goToZomiePage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/achivzomb" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="545dp"
        android:layout_height="111dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="20dp"
        android:background="@color/DarckDrown"
        android:onClick="goToPlantPage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton"
        app:srcCompat="@drawable/achivnut" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="512dp"
        android:layout_height="111dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="22dp"
        android:background="@color/DarckDrown"
        android:onClick="goToZomiePage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton4"
        app:srcCompat="@drawable/achivzombeti" />
</androidx.constraintlayout.widget.ConstraintLayout>